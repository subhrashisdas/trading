{
  "version": "0.1.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "tsdx lint src test --fix"
  },
  "eslintConfig": {
    "plugins": [
      "sort-imports-es6-autofix"
    ],
    "extends": [
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "sort-imports-es6-autofix/sort-imports-es6": [
        2,
        {
          "ignoreCase": false,
          "ignoreMemberSort": false,
          "memberSyntaxSortOrder": [
            "none",
            "all",
            "multiple",
            "single"
          ]
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/src/$1",
      "^@test/(.*)$": "<rootDir>/test/$1"
    },
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/test/all.test.ts"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@types/lodash": "^4.14.165",
    "@types/node-fetch": "^2.5.7",
    "commander": "^6.2.0",
    "date-fns": "^2.16.1",
    "got": "^11.8.0",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1"
  }
}
